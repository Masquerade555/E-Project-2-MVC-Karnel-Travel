                                        }
                                        else
                                        {
                                            <a class="Add" data-id="@item.h_id">ADD TO CART</a>
                                        }
                                    </div>
                                    <a href="#" class="like"><i class="fa fa-heart-o"></i></a>
                                </li>

                            }
                            


                        </ul>
                    </div>
                </div>
            </div><!-- /.row -->
        </div><!-- /.container -->
    </section>
    <!-- END PRODUCT NEW -->
}
else
{
    <h5 class="mt-5 text-center font-weight-bold">Oops!</h5>
    <h3 class="mt-5 mb-5 text-center">Products Not Found.</h3>
}
