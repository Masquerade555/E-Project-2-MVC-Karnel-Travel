                                            <i class="fa fa-star"></i>
                                            <i class="fa fa-star"></i>
                                            <i class="fa fa-star-half-o"></i>
                                        </div>
                                       
                                    </div>
                                    <div class="add-to-cart text-center">
                                        @if (Session["u_id"] == null)
                                        {
                                            <a>ADD TO CART</a>
                                        }
                                        else
